{
    "name": "alexanderglueck/contacts",
    "description": "A simple contact management system to keep track of your friends and family.",
    "keywords": ["contacts", "laravel"],
    "license": "TBD",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "babenkoivan/scout-elasticsearch-driver": "^3.10",
        "barryvdh/laravel-ide-helper": "^2.4",
        "bugsnag/bugsnag-laravel": "^2.12",
        "cviebrock/eloquent-sluggable": "^4.5",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "~4.0",
        "globalcitizen/php-iban": "^2.6",
        "intervention/image": "^2.4",
        "jeroendesloovere/vcard": "^1.5",
        "laravel/cashier": "~9.3",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.2",
        "laravel/tinker": "~1.0",
        "maatwebsite/excel": "^3.1",
        "mpociot/teamwork": "^5.1",
        "pragmarx/google2fa": "^3.0",
        "spatie/laravel-permission": "^2.9"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/utilities/functions.php"]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
